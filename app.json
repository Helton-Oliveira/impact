{
  "expo": {
    "name": "mobile",
    "slug": "mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./mobile/assets/images/favicon.png",
    "scheme": "digisphere",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./mobile/assets/images/favicon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.mobile"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./mobile/assets/images/favicon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.anonymous.mobile"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./mobile/assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-router",
        {
          "root": "./mobile/app"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "mobileRoot": "mobile"
    }
  }
}